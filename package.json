{
  "name": "custreprstatusui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:web": "react-scripts start",
    "prod:web": "react-scripts build",
    "electron": "electron .",
    "dev:ele": "concurrently \"BROWSER=none yarn dev:web\" \"wait-on http://localhost:3000 && yarn electron\"",
    "prod:ele": "yarn prod:web && yarn dist",
    "dist": "electron-builder -w",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle": ""
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "16.8.3",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "0.59.5",
    "react-native-cli": "^2.0.1",
    "react-native-web": "^0.11.2",
    "react-scripts": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "concurrently": "^4.1.0",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3",
    "typescript": "^3.4.4",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "preset": "react-native-web"
  },
  "build": {
    "appId": "com.alfame.custreprstatusui",
    "mac": {
      "category": "tools"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
